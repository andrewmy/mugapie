{
    "type": "project",
    "license": "proprietary",
    "name": "andrewmy/mugapie",
    "description": "A sample JSON REST API project",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/core": "^2.1",
        "barbieswimcrew/zip-code-validator": "^1.3",
        "beberlei/assert": "^3.2",
        "doctrine/collections": "^1.6",
        "doctrine/dbal": "^2.10",
        "doctrine/doctrine-bundle": "2.6.4",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/event-manager": "^1.1",
        "doctrine/migrations": "^2.2",
        "doctrine/orm": "^2.5.11",
        "doctrine/persistence": "^2",
        "kucharovic/money-bundle": "^1.3",
        "moneyphp/money": "^3.3",
        "nelmio/cors-bundle": "^1.5 || ^2.0",
        "nesbot/carbon": "^2.29",
        "psr/log": "^1.1",
        "ramsey/uuid": "^3.9",
        "ramsey/uuid-doctrine": "^1.6",
        "symfony/asset": "5.4.*",
        "symfony/config": "5.4.*",
        "symfony/contracts": "^2.0",
        "symfony/dependency-injection": "5.4.*",
        "symfony/doctrine-bridge": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/event-dispatcher": "5.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-kernel": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/routing": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/yaml": "5.4.*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.3",
        "dms/phpunit-arraysubset-asserts": "^0.4.0",
        "doctrine/coding-standard": "^7.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "infection/infection": "^0.15.0",
        "insolita/unused-scanner": "^2.1",
        "maglnet/composer-require-checker": "^2.1",
        "mockery/mockery": "^1.3",
        "phpstan/phpstan": "^1",
        "phpstan/phpstan-deprecation-rules": "^1",
        "phpstan/phpstan-doctrine": "^1",
        "phpstan/phpstan-mockery": "^1",
        "phpstan/phpstan-strict-rules": "^1",
        "phpunit/phpunit": "^8.5",
        "psalm/plugin-mockery": "^0.3.0",
        "qossmic/deptrac-shim": "^0.24.0",
        "roave/security-advisories": "dev-latest",
        "rregeer/phpunit-coverage-check": "^0.3.1",
        "symfony/browser-kit": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/phpunit-bridge": "^5.0",
        "thibautselingue/local-php-security-checker-installer": "^1.0",
        "vimeo/psalm": "^3.8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
